01 
    const int pwmR = 255;     //PWM PARA QUE VAYA RECTO

    const int pwmA1 = 255;     //CURVA A
    const int pwmA2 = 240;

    const int pwmB1 = 250;     //CURVA B
    const int pwmB2 = 230;

    const int pwmC1 = 220;      //CURVA C
    const int pwmC2 = 190;

    const int pwmD1 = 220;      //CURVA D
    const int pwmD2 = 160;

    const int pwmE1 = 220;      //CURVA E
    const int pwmE2 = 110;

    const int pwmF1 = 200;      //CURVA F
    const int pwmF2 = 70;

    const int pwmG1 = 190;      //CURVA G
    const int pwmG2 = 20;

